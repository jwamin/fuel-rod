{
  "name": "reactor",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "config": {
    "source_dir": "./src/source.js",
    "target_dir": "./build/build.js",
    "babel_opts": "--module-bind js=babel?presets[]=es2015,presets[]=react,plugins[]=transform-object-rest-spread,ignore=node_modules,cacheDirectory",
    "webpack_opts": "--devtool inline-source-map",
    "presets": {
      "react": [
        "es2015",
        "react"
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  },
  "scripts": {
    "build": "webpack $npm_package_config_webpack_opts $npm_package_config_babel_opts $npm_package_config_source_dir $npm_package_config_target_dir",
    "build2": "$npm_package_config_presets_react | json && webpack $npm_package_config_webpack_opts --module-bind js=babel --presets $npm_package_config_presets_react --plugins npm_package_config_presets_plugins $npm_package_config_source_dir $npm_package_config_target_dir",
    "build3":"bash script.sh build",
    "json": "REACT=$(cat package.json | json config.presets.react) && echo $REACT",
    "watch": "webpack watch $npm_package_config_webpack_opts $npm_package_config_babel_opts $npm_package_config_source_dir",
    "test": "webpack --devtool inline-source-map --module-bind js=babel?presets[]=es2015,presets[]=react,plugins[]=transform-object-rest-spread,ignore=node_modules,cacheDirectory ./src/source.js ./build/build.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "parse-json": "^2.2.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  }
}
